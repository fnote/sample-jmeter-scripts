          <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="set date" enabled="true">
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import java.text.ParseException;
import java.text.SimpleDateFormat;
import java.time.LocalDate;
import java.util.Date;
import java.util.concurrent.ThreadLocalRandom;
import java.util.Calendar;

LocalDate today = LocalDate.now();
LocalDate startDate = today.plusDays(3);
long start = startDate.toEpochDay();
LocalDate endDate = today.plusDays(90);
long end = endDate.toEpochDay();

long randomEpochDay = ThreadLocalRandom.current().longs(start, end).findAny().getAsLong();
String epochDate = String.valueOf(LocalDate.ofEpochDay(randomEpochDay));

SimpleDateFormat df_in = new SimpleDateFormat(&quot;yyyy-MM-dd&quot;);
SimpleDateFormat df_output = new SimpleDateFormat(&quot;yyyyMMdd&quot;);
Date date = df_in.parse(epochDate);
String randomDate = df_output.format(date);
Calendar c = Calendar.getInstance();
c.setTime(date);
c.add(Calendar.DATE, 14);
Date endDay = c.getTime();
String randomDate2 = df_output.format(endDay);

vars.put(&quot;effectiveFromDate&quot;, randomDate);
vars.put(&quot;endDate&quot;, randomDate2);</stringProp>
          </BeanShellPreProcessor>
